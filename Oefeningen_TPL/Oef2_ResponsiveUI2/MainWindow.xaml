        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oef2_ResponsiveUI2"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Start" HorizontalAlignment="Left" Height="33" Margin="58,116,0,0" VerticalAlignment="Top" Width="100" RenderTransformOrigin="0.505,1.855" Click="button_Click"/>
        <ProgressBar x:Name="progressBar" HorizontalAlignment="Left" Height="32" Margin="58,218,0,0" VerticalAlignment="Top" Width="389" RenderTransformOrigin="0.496,1.506"/>
        <Button x:Name="button1" Content="Stop" HorizontalAlignment="Left" Margin="348,116,0,0" VerticalAlignment="Top" Width="99" Height="33" Click="button1_Click"/>
    </Grid>
</Window>
