<Window x:Class="Oef2_DockPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oef2_DockPanel"
        mc:Ignorable="d"
        Title="Oefening 2 - DockPanel" Height="350" Width="525">
    <DockPanel>
        <Button Content="Knop 1" DockPanel.Dock="Top" Height="50" />
        <Button Content="Knop 2" DockPanel.Dock="Right" Width="50" />
        <Button Content="Knop 3" DockPanel.Dock="Bottom" Height="50" />
        <Button Content="Knop 4" DockPanel.Dock="Left" Width="50" />
        <Button Content="Knop 5" VerticalAlignment="Top" Height="50"/>
    </DockPanel>
</Window>
