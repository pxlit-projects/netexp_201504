<Application x:Class="GuestRoomWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GuestRoomWPF"
             xmlns:converter="clr-namespace:GuestRoomWPF.Converter"
             StartupUri="View/GRStartView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <local:ViewModelLocator x:Key="mainViewModelLocator"></local:ViewModelLocator>
            <converter:ImageConverter x:Key="localImageConverter"></converter:ImageConverter>
        </ResourceDictionary>
    </Application.Resources>
</Application>
